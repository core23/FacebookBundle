<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <parameters>
    <parameter key="core23_facebook.connection.class">Core23\FacebookBundle\Connection\FacebookConnection</parameter>
  </parameters>
  <services>
    <service id="core23_facebook.connection" class="%core23_facebook.connection.class%">
      <argument>%core23_facebook.api.app_id%</argument>
      <argument>%core23_facebook.api.app_secret%</argument>
    </service>
  </services>
</container>
