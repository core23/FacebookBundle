<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="core23_facebook.action.start_auth" class="Core23\FacebookBundle\Action\StartAuthAction">
      <argument type="service" id="router"/>
      <argument type="service" id="core23_facebook.connection"/>
      <argument>%core23_facebook.api.permissions%</argument>
    </service>
    <service id="core23_facebook.action.auth_error" class="Core23\FacebookBundle\Action\AuthErrorAction">
      <argument type="service" id="twig"/>
      <argument type="service" id="router"/>
      <argument type="service" id="core23_facebook.session.manager"/>
      <argument type="service" id="event_dispatcher"/>
    </service>
    <service id="core23_facebook.action.auth_success" class="Core23\FacebookBundle\Action\AuthSuccessAction">
      <argument type="service" id="twig"/>
      <argument type="service" id="router"/>
      <argument type="service" id="core23_facebook.session.manager"/>
      <argument type="service" id="event_dispatcher"/>
    </service>
    <service id="core23_facebook.action.check_auth" class="Core23\FacebookBundle\Action\CheckAuthAction">
      <argument type="service" id="router"/>
      <argument type="service" id="core23_facebook.connection"/>
      <argument type="service" id="core23_facebook.session.manager"/>
    </service>
  </services>
</container>
