<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <parameters>
    <parameter key="core23.facebook.block.facebook_timeline.class">Core23\FacebookBundle\Block\Service\FacebookTimelineBlockService</parameter>
  </parameters>
  <services>
    <service id="core23.facebook.block.facebook_timeline" class="%core23.facebook.block.facebook_timeline.class%">
      <tag name="sonata.block"/>
      <argument>core23.facebook.block.facebook_timeline</argument>
      <argument type="service" id="templating"/>
      <argument>%core23_facebook.api%</argument>
      <call method="setLogger">
        <argument type="service" id="logger"/>
      </call>
    </service>
  </services>
</container>
