{
    "name": "core23/facebook-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a block service for using the Facebook API inside the sonata-project.",
    "keywords": [
        "symfony",
        "bundle",
        "facebook",
        "sonata",
        "api"
    ],
    "homepage": "https://core23.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "require": {
        "php": "^7.2",
        "core23/antispam-bundle": "^0.2 || ^0.3 || ^0.4",
        "core23/twig-extensions": "^0.3 || ^1.0",
        "facebook/graph-sdk": "^5.0",
        "psr/log": "^1.0",
        "sonata-project/core-bundle": "^3.13.5",
        "symfony/config": "^3.4 || ^4.2",
        "symfony/dependency-injection": "^3.4 || ^4.2",
        "symfony/event-dispatcher": "^3.4 || ^4.2",
        "symfony/expression-language": "^3.4 || ^4.2",
        "symfony/form": "^3.4 || ^4.2",
        "symfony/framework-bundle": "^3.4 || ^4.2",
        "symfony/http-foundation": "^3.4 || ^4.2",
        "symfony/http-kernel": "^3.4 || ^4.2",
        "symfony/routing": "^3.4 || ^4.2",
        "symfony/twig-bundle": "^3.4 || ^4.2",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.2",
        "sllh/composer-lint": "^1.0",
        "sonata-project/admin-bundle": "^3.35",
        "sonata-project/block-bundle": "^3.13",
        "symfony/options-resolver": "^3.4 || ^4.2"
    },
    "suggest": {
        "sonata-project/block-bundle": "If you want to use sonata blocks."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Core23\\FacebookBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Core23\\FacebookBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ]
    }
}
