{
  "name": "core23/facebook-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides a block service for using the Facebook API inside the sonata-project.",
  "keywords": [
    "symfony",
    "bundle",
    "facebook",
    "sonata",
    "api"
  ],
  "homepage": "https://core23.de",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Gripp",
      "email": "mail@core23.de"
    }
  ],
  "require": {
    "php": "^7.1",
    "facebook/graph-sdk" : "^5.0",
    "sonata-project/core-bundle": "^3.0",
    "symfony/dependency-injection": "^2.8.6 || ^3.2 || ^4.0",
    "symfony/expression-language": "^2.8.6 || ^3.2 || ^4.0",
    "symfony/form": "^2.8.6 || ^3.2 || ^4.0",
    "symfony/framework-bundle": "^2.8.6 || ^3.2 || ^4.0"
  },
  "require-dev": {
    "matthiasnoback/symfony-dependency-injection-test": "^1.1",
    "phpunit/phpunit": "^5.7",
    "sebastian/exporter": "^2.0",
    "sonata-project/admin-bundle": "^3.1",
    "sonata-project/block-bundle": "^3.3",
    "symfony/phpunit-bridge": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Core23\\FacebookBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Core23\\FacebookBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
